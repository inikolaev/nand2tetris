/**
 * Computes the sum of two bits.
 */

CHIP And3Way {
    IN a, b, c; // 1-bit inputs
    OUT out;    // a & b & c

    PARTS:
    // Put you code here:
    And(a=a, b=b, out=c0);
    And(a=c0, b=c, out=out);
}
